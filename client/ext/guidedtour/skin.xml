<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml">
<a:window name="qsImage">
        <a:style><![CDATA[
            .qsProjectBar {
                position:absolute;
                top: 0;
                left: 45px;
                width: 167px;
                height: 117px;
                background: url(images/your_project_bar.png) no-repeat 0 0;
            }
        ]]></a:style>
    
        <a:presentation>
        <a:main
              collapsed-height = "30"
              minheight        = "30"
              minwidth         = "60"
              container        = "div[2]"
              drag             = "div[2]"
              title            = "div[1]/div[1]/text()">
                <div>
                    <div>
                    </div>
                    <div class="image"> </div>
                </div>
            </a:main>
        </a:presentation>
    </a:window>
    
    <a:window name="quickstart">
        <a:style><![CDATA[
            .quickstart {
                position : relative;
                overflow : visible;
                z-index  : 10000;
                box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.25);
                border-radius: 4px;
            }
            
            .quickstart .header {
                width: 20px;
                height: 20px;
                position: absolute;
                top: -22px;
                right: 0;
                background: url(images/close.png) no-repeat 0 0;
            }
            .quickstart .header:hover {
                background-position: -22px 0px;
            }
            .quickstart .header:active {
                background-position: -44px 0px;
            }
            
            .quickstart .content {
                background : #000;
                color: #fff;
                border : 1px solid #999;
                border-radius: 4px;
                padding: 10px 15px;
                text-align: center;
            }
            
            .quickstart .buttons {
                background: url(images/close.png) no-repeat 0 0;
            }
            
            .qstitle, .tgtext, .qsSupportText, .tgLink {
                background : #000;
                color: #fff;
                font-size: 16px;
            }
            
            .tgtext {
                color: orange;
            }
            
            .qsSupportText {
                font-size: 12px;
            }
            
            .supportLink {
                color: #6495ED;
            }
        ]]></a:style>
    
        <a:presentation>
        <a:main
              collapsed-height = "30"
              minheight        = "30"
              minwidth         = "60"
              container        = "div[2]"
              drag             = "div[2]"
              title            = "div[1]/div[1]/text()">
                <div class="quickstart">
                    <div class="header" onclick="require('ext/guidedtour/guidedtour').closeStart();">
                    </div>
                    <div class="content"> </div>
                </div>
            </a:main>
        </a:presentation>
    </a:window>
     
    <a:window name="guidetool">
        <a:style><![CDATA[
            .guidetool {
                position : relative;
                overflow : visible;
                z-index  : 10000;
                box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.25);
                border-radius: 4px;
            }
            .guidetool .gt-header {
                display : none;
            }
            .guidetool .gt-content {
                background : #fff;
                border : 1px solid #999;
                border-radius: 4px;
                padding: 10px 15px;
                text-align: center;
            }
            .guidetool .gt-content .text {
                color: #555;
            }
            .guidetool .gt-arrow {
                position: absolute;
                width : 16px;
                height : 16px;
                background : #fff;
                border: 1px solid #999;
            }
            .guidetool.top .gt-arrow {
                left: 50%;
                margin-left: -8px;
                margin-top: -1px;
                border-top: none;
            }
            .guidetool.bottom .gt-arrow {
                left: 50%;
                margin-left: -8px;
                top: 0;
                margin-top: -15px;
                border-bottom: none;
            }
            .guidetool.left .gt-arrow {
                right: 0px;
                top: 50%;
                margin-top: -8px;
                margin-left: 16px;
                border-left: none;
            }
            .guidetool.right .gt-arrow {
                left: 0px;
                top: 50%;
                margin-top: -8px;
                margin-left: -16px;
                border-right: none;
            }
            
            .guidetool .btn {
                color: red;
            }
        ]]></a:style>
    
        <a:presentation>
            <a:main
              collapsed-height = "30"
              minheight        = "30"
              minwidth         = "60"
              container        = "div[2]"
              drag             = "div[2]"
              title            = "div[1]/div[1]/text()"
              icon             = "div[1]/div[1]/div[1]"
              buttons          = "div[1]/div[1]/div[2]">
                <div class="guidetool">
                    <div class="gt-header">
                        <div class="hcontent">-<div class="icon"></div><div class="buttons"></div></div>
                        <div class="right"></div>
                    </div>
                    <div class="gt-content"> </div>
                    <div class="gt-arrow"></div>
                </div>
            </a:main>
            <a:button>
                <div> </div>
            </a:button>
        </a:presentation>
    </a:window>
</a:skin>